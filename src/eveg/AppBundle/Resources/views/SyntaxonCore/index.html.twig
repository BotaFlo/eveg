{% extends "evegAppBundle:Admin:layout.html.twig" %}

{% block body -%}
    <h1>SyntaxonCore list</h1>

    <table class="records_list">
        <thead>
            <tr>
                <th>Id</th>
                <th>Fixedcode</th>
                <th>Catminatcode</th>
                <th>Level</th>
                <th>Syntaxonname</th>
                <th>Syntaxonauthor</th>
                <th>Commonname</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td><a href="{{ path('admin_syntaxon_show', { 'id': entity.id }) }}">{{ entity.id }}</a></td>
                <td>{{ entity.fixedCode }}</td>
                <td>{{ entity.catminatCode }}</td>
                <td>{{ entity.level }}</td>
                <td>{{ entity.syntaxonName }}</td>
                <td>{{ entity.syntaxonAuthor }}</td>
                <td>{{ entity.commonName }}</td>
                <td>
                <ul>
                    <li>
                        <a href="{{ path('admin_syntaxon_show', { 'id': entity.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('admin_syntaxon_edit', { 'id': entity.id }) }}">edit</a>
                    </li>
                </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

        <ul>
        <li>
            <a href="{{ path('admin_syntaxon_new') }}">
                Create a new entry
            </a>
        </li>
    </ul>
    {% endblock %}
